@model TestIdentitySmaple.Models.EncryptionTestViewModel

@{
    ViewBag.Title = "AES Encyption";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>AES Encyption</h2>
@using (Html.BeginForm("AES", "TestEncyption", FormMethod.Post, new {id = "frm", @class="form-horizontal"}))
{
    <div class="form-group">
        @Html.LabelFor(model=>model.Source)
        <div class="col-md-10">
            @Html.TextBoxFor(model=>model.Source)
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(model=>model.Destination)
        <div class="col-md-10">
            @Html.TextBoxFor(model=>model.Destination)
        </div>
    </div>
    <div  class="well-sm">
        <input type="submit" class="btn btn-success" value="Save"/>
    </div>
}